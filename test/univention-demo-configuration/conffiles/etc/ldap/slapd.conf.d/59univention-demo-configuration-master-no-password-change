@!@
INDENT = f'  %s'
TO_CLAUSE = f'access to\n%s' % INDENT
BY_CLAUSE = INDENT % f'by %s '

ldap_base = configRegistry['ldap/base']
print(TO_CLAUSE % f'dn="uid=Administrator,cn=users,{ldap_base}"')
print(INDENT % 'attrs="krb5Key,userPassword,sambaPwdCanChange,sambaPwdMustChange,sambaLMPassword,sambaNTPassword,sambaPwdLastSet,pwhistory,sambaPasswordHistory,krb5KDCFlags,krb5KeyVersionNumber,krb5PasswordEnd,shadowMax,shadowLastChange,uid,cn,entry"')
print(BY_CLAUSE % 'self read')
print(BY_CLAUSE % f'group/univentionGroup/uniqueMember="cn=Domain Admins,cn=groups,{ldap_base}" read')
print(BY_CLAUSE % '* none break')
print('')
@!@
